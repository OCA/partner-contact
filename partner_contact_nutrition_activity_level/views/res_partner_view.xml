<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_partner_contact_activity_level" model="ir.ui.view">
        <field name="name">Partner Contact Activity Level</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="partner_contact_nutrition.view_nutrition_information"/>
        <field name="arch" type="xml">
            <xpath expr="//group[@name='other_info']" position="inside">
                <field name="activity_level"/>
            </xpath>
        </field>
    </record>

</odoo>
