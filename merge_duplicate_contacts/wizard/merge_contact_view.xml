                    </group>
                    <group>
                        <label for="vat_1" style="min-width: 108px;" />
                        <div>
                            <field
                                name="vat_1"
                                nolabel="1"
                                class="oe_inline"
                                style="min-width: 300px;"
                                attrs="{'readonly': [('contact_type', '=', 'contact'),('company_id', '!=', False)]}"
                            />
                            <field
                                name='vat_show_icon'
                                invisible="1"
                                class="oe_inline"
                            />
                            <button
                                name="swap_to_left"
                                type="object"
                                title="left"
                                class="fa fa-arrow-left oe_inline"
                                context="{'field_name':'vat_2'}"
                            />

                            <button
                                name="dummy_button"
                                string="Differ"
                                type="object"
                                class="btn-danger oe_inline"
                                attrs="{'invisible':[('vat_show_icon','=',True)]}"
                            />
                            <button
                                name="dummy_button"
                                string="Same"
                                type="object"
                                class="oe_inline"
                                attrs="{'invisible':[('vat_show_icon','=',False)]}"
                            />

                            <button
                                name="swap_to_right"
                                type="object"
                                title="right"
                                class="fa fa-arrow-right oe_inline"
                                context="{'field_name':'vat_1'}"
                            />
                            <field
                                name="vat_2"
                                nolabel="1"
                                class="oe_inline"
                                style="min-width: 300px;"
                                attrs="{'readonly': [('contact_type2', '=', 'contact'),('company_id2', '!=', False)]}"
                            />
                        </div>
                    </group>
                    <group>
                        <label for="is_company" style="min-width: 108px;" />
                        <div>
                            <field
                                name="is_company"
                                nolabel="1"
                                class="oe_inline"
                                style="min-width: 300px;"
                            />
                            <field
                                name='is_company_show_icon'
                                invisible="1"
                                class="oe_inline"
                            />
                            <button
                                name="swap_to_left"
                                type="object"
                                title="left"
                                class="fa fa-arrow-left oe_inline"
                                context="{'field_name':'is_company2'}"
                            />

                            <button
                                name="dummy_button"
                                string="Differ"
                                type="object"
                                class="btn-danger oe_inline"
                                attrs="{'invisible':[('is_company_show_icon','=',True)]}"
                            />
                            <button
                                name="dummy_button"
                                string="Same"
                                type="object"
                                class="oe_inline"
                                attrs="{'invisible':[('is_company_show_icon','=',False)]}"
                            />

                            <button
                                name="swap_to_right"
                                type="object"
                                title="right"
                                class="fa fa-arrow-right oe_inline"
                                context="{'field_name':'is_company'}"
                            />
                            <field
                                name="is_company2"
                                nolabel="1"
                                class="oe_inline"
                                style="min-width: 300px;"
                            />
                        </div>
                    </group>
                    <group col="16">
                        <label colspan="1" for="keep1" style="min-width: 108px;" />
                        <field colspan="5" name="keep1" nolabel="1" />
                        <button
                            colspan="1"
                            name="swap_to_left"
                            type="object"
                            title="left"
                            class="fa fa-arrow-left"
                            style="display:none;"
                        />
                        <button
                            colspan="1"
                            name="swap_to_right"
                            type="object"
                            title="right"
                            class="fa fa-arrow-right"
                            style="display:none;"
                        />
                        <field colspan="5" name="keep2" nolabel="1" />
                    </group>
                </sheet>
                <footer>
                    <center>
                        <button
                            name="action_merge"
                            string="Merge"
                            type="object"
                            style="margin-right:100px;"
                        />
                        <button
                            name="action_skip"
                            string="Skip"
                            type="object"
                            style="margin-right:100px;"
                        />
                        <button
                            special="cancel"
                            string="Cancel"
                            type="object"
                            class="btn btn-secondary oe_inline"
                            style="margin-right:100px;"
                        />
                    </center>
                </footer>
            </form>
        </field>
    </record>

