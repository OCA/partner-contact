<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="partner_number_cron" model="ir.cron">
        <field name="name">Partner Identification Expiration Checking</field>
        <field
            name="model_id"
            ref="partner_identification.model_res_partner_id_number"
        />
        <field name="state">code</field>
        <field name="code">model.check_valid_until_date()</field>
        <field name="user_id" ref="base.user_root" />
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
        <field name="active" eval="True" />
    </record>

</odoo>
