<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record model="ir.ui.view" id="view_country_geonames_form">
        <field name="name">res.country.geonames.form</field>
        <field name="model">res.country</field>
        <field name="inherit_id" ref="base.view_country_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <field name="code" position="after">
                <field name="geonames_state_name" />
                <field name="geonames_state_code" />
            </field>
        </field>
    </record>

    <menuitem id="better_zip_country_geonames_import_menu"
        action="base.action_country"
        parent="base_location.locations_root_menu"
        sequence="25"/>
</odoo>


